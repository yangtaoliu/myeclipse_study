<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.study.elec.domain.ElecText" table="Elec_Text">
		<id name="textID" type="string">
			<generator class="uuid"></generator>
		</id>
		<property name="textName"  type="string"></property>
		<property name="textDate" type="date"></property>
		<property name="textRemark" type="string"></property>
	</class>
</hibernate-mapping>